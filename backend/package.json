{
  "name": "my-package-manager",
  "version": "1.0.0",
  "description": "A package manager that ranks packages based on various factors.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "deepclean": "rm -rf node_modules && rm -f package-lock.json && npm cache clean --force",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "es6-promise": "^4.2.8",
    "express": "^4.21.0",
    "isomorphic-git": "^1.27.1",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.4",
    "@types/semver": "^7.5.8",
    "commander": "^12.1.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
