{
  "name": "my-package-manager",
  "version": "1.0.0",
  "description": "A package manager that ranks packages based on various factors.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
